	//项目操作区域按钮事件
	方法 项目操作区域按钮事件()
		变量 项目路径 : 文本 = 其他_变量.项目目录 + 项目名称
		//检查
		如果 文件是否存在(其他_变量.备份目录 + 项目名称 + ".mcides") 则
			文本框52.内容 = "上次:" + 取文件修改时间(其他_变量.备份目录 + 项目名称 + ".mcides")
			文本框55.内容 = "请确保你已经备份了"
		否则
			文本框52.内容 = "没有最近的备份记录"
			文本框55.内容 = "检测到你的项目没有备份"
		结束 如果
		如果 文件是否存在(其他_变量.缓存目录 + "shares/" + 项目名称 + ".mcides") 则
			文本框56.内容 = "上次于" + 取文件修改时间(其他_变量.缓存目录 + "shares/" + 项目名称 + ".mcides") + "时分享"
		否则
			文本框56.内容 = "没有任何的分享记录!"
		结束 如果
		//备份
		事件 相对布局19:被单击()
			如果 压缩操作.ZIP压缩(项目路径,其他_变量.备份目录 + 项目名称 + ".mcides") 则
				弹出成功提示("备份成功")
				文本框55.内容 = "请确保你已经备份了"
				文本框52.内容 = "最近:" + 取文件修改时间(其他_变量.备份目录 + 项目名称 + ".mcides")
			否则
				弹出错误提示("备份失败")
				文本框52.内容 = "备份失败!"
			结束 如果
		结束 事件
		//分享
		事件 相对布局20:被单击()
			写出文本文件(其他_变量.缓存目录 + "shares/null","")
			删除文件(其他_变量.缓存目录 + "shares/null")
			如果 压缩操作.ZIP压缩(项目路径,其他_变量.缓存目录 + "shares/" + 项目名称 + ".mcides") 则
				文本框56.内容 = 取文件修改时间(其他_变量.缓存目录 + "shares/" + 项目名称 + ".mcides") + "时分享"
				分享操作.分享文件(本对象,其他_变量.缓存目录 + "shares/" + 项目名称 + ".mcides")
			否则
				弹出错误提示("尝试分享失败")
			结束 如果
		结束 事件
		//删除
		事件 相对布局21:被单击()
			变量 删除提示 : DX基础对话框
			如果 设置_设定.获取开关("NightMode") 则
				删除提示.主题(DX主题.暗色)
			结束 如果
			删除提示.标题("删除")
			删除提示.内容("删除该项目?\n" + 项目名称)
			删除提示.按钮1("确定")
			删除提示.按钮2("取消")
			事件 删除提示:按钮1被单击() : 逻辑型
				如果 文件操作.删除文件(项目路径) 则
					弹出成功提示("删除项目成功")
					关闭窗口()
				否则
					弹出错误提示("删除项目失败")
				结束 如果
				返回 假
			结束 事件
			删除提示.显示()
		结束 事件
	结束 方法

